{
    "name": "@developjik1/prettier-config-developjik1",
    "version": "0.0.1",
    "private": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
      "build": "tsup index.js --format cjs,esm --dts"
    },
    "devDependencies": {
        "prettier": "^3.5.3",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    }
}